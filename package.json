{
  "name": "emilioocelotl.github.io",
  "version": "1.0.0",
  "description": "Sitio web y generador de portafolio PDF",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./",
    "pdf": "node src/generate-portfolio.js",
    "pdf:dev": "node --watch src/generate-portfolio.js",
    "pdf:clean": "rm -f portfolio-*.pdf",
    "pdf:all": "npm run pdf && echo 'âœ… PDF generado en portfolio-artistico.pdf'",
    "dev": "npm run start & npm run pdf:dev",
    "deploy": "npm run build && npm run pdf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilioOcelotl/emilioocelotl.github.io.git"
  },
  "author": "Emilio Ocelotl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EmilioOcelotl/emilioocelotl.github.io/issues"
  },
  "homepage": "https://github.com/EmilioOcelotl/emilioocelotl.github.io#readme",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "hydra-synth": "^1.3.29",
    "parcel": "^2.12.0",
    "pdfkit": "^0.14.0",
    "plyr": "^3.7.8",
    "three": "^0.164.1"
  },
  "devDependencies": {
    "events": "^3.3.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "process": "^0.11.10"
  },
  "pdfConfig": {
    "outputDir": "./",
    "filename": "portfolio-artistico.pdf",
    "maxProjects": 9
  },
  "files": [
    "dist/",
    "src/",
    "package.json",
    "README.md"
  ]
}
